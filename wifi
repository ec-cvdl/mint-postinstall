# Créer une fausse connexion WiFi pour tester
sudo nmcli con add type wifi ifname wlan0 con-name "Test-WiFi" ssid "MonReseauTest"
sudo nmcli con modify "Test-WiFi" wifi-sec.key-mgmt wpa-psk wifi-sec.psk "motdepasse123"

# Vérifier qu'elle est créée
sudo ls /etc/NetworkManager/system-connections/ | grep Test

# Puis tester votre script de suppression

# Nettoyer après le test
sudo nmcli con delete "Test-WiFi"
